{
  "extends": "tslint:recommended",
  "defaultSeverity": "error",
  "rules": {
    "no-console": {
      "severity": "warning",
      "options": ["debug", "info", "log", "trace"]
    },
    "no-empty": true,
    "no-unused-variable": true,
    "quotemark": [true, "single"],
    "semicolon": true
  },
  "jsRules": {}
}
